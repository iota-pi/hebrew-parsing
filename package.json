{
  "name": "hebrew-parsing",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "upload": "aws s3 cp build/ s3://hebrew-parsing-production/ --recursive --acl public-read || true",
    "deploy:app": "yarn build && yarn upload",
    "deploy": "yarn deploy:app && cd infra && terraform apply",
    "lint": "yarn run eslint src --ext .ts,.tsx",
    "start": "vite",
    "preview": "vite preview",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.15.12",
    "@mui/styles": "^5.15.12",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.2",
    "usehooks-ts": "^3.0.1",
    "vite": "^5.0.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-env": "^7.0.3",
    "tsx": "^4.7.0"
  },
  "packageManager": "yarn@4.0.2"
}
