{
  "name": "hebrew-parsing",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "upload": "aws s3 cp build/ s3://bgf-app-production/ --recursive --acl public-read",
    "deploy:app": "yarn build && yarn upload",
    "build:lambda": "esbuild lambda/index.ts --outfile=lambda/build/lambda.js --bundle --minify --platform=node --target=node14 --external:aws-sdk --external:canvas",
    "bundle:lambda": "cd lambda/build && 7z a -tzip lambda.zip lambda.js",
    "upload:lambda": "aws s3 cp lambda/build/lambda.zip s3://crosscode-lambdas/bgf/lambda.zip",
    "deploy:lambda": "yarn build:lambda && yarn bundle:lambda && yarn upload:lambda",
    "deploy": "yarn deploy:app && yarn deploy:lambda && cd infra && terraform apply",
    "lint": "yarn run eslint src lambda --ext .ts,.tsx",
    "loadtest": "yarn tsx lambda/loadtest",
    "api": "cd lambda && yarn tsx local",
    "start": "vite",
    "preview": "vite preview",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.485.0",
    "@aws-sdk/client-schemas": "^3.485.0",
    "@aws-sdk/core": "^3.485.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@loadable/component": "^5.15.0",
    "@mui/lab": "^5.0.0-alpha.167",
    "@mui/material": "^5.15.12",
    "@mui/styles": "^5.15.12",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.25.0",
    "@trpc/client": "^11.0.0-next-beta.311",
    "@trpc/react-query": "^11.0.0-next-beta.316",
    "@trpc/server": "^11.0.0-next-beta.311",
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^16.2.13",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.22.2",
    "redux": "^5.0.1",
    "typescript": "^5.4.2",
    "vite": "^5.0.10",
    "zod": "^3.22.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^1.5.3",
